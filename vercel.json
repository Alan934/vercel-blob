{
    "routes": [
      {
        "src": "/(.*\\.(jpg|jpeg|png|gif|svg))$",
        "headers": {
          "Cache-Control": "no-cache"
        },
        "dest": "/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  